<%= form_for :assets, url: assets_path(@asset), method: :patch do |f| %>
  <p>
    <%= f.label :manufacturer %>
    <%= f.text_field :manufacturer %>
  </p>
 
  <p>
    <%= f.label :model %>
    <%= f.text_field :model %>
  </p>

  <p>
    <%= f.label :JSLG_Tag %>
    <%= f.text_field :JSLG_Tag %>
  </p>

  <p>
    <%= f.label :service_tag %>
    <%= f.text_field :service_tag %>
  </p>

  <p>
    <%= f.label :serial_number %>
    <%= f.text_field :serial_number %>
  </p>

  <p>
    <%= f.label :os_type %>
    <%= f.text_field :os_type %>
  </p>

  <p>
    <%= f.label :host_name %>
    <%= f.text_field :host_name %>
  </p>

  <p>
    <%= f.label :ram %>
    <%= f.text_field :ram %>
  </p>

  <p>
    <%= f.label :hdd %>
    <%= f.text_field :hdd %>
  </p>

  <p>
    <%= f.label :department %>
    <%= f.text_field :department %>
  </p>

  <p>
    <%= f.label :location %>
    <%= f.text_field :location %>
  </p>

  <p>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </p>

  <p>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </p>

  <p>
    <%= f.label :status %>
    <%= f.text_field :status %>
  </p>

  <p>
    <%= f.label :notes %>
    <%= f.text_field :notes %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', assets_path %>